// ObjectsMatrixBitMap File 
// A two level bitmap. dosplaying harts on the screen Apr  2023  
// (c) Technion IIT, Department of Electrical Engineering 2023 
module	ObjectsMatrixBitMap	(	
					input	logic	clk,
					input	logic	resetN,
					input logic	[10:0] offsetX,// offset from top left  position 
					input logic	[10:0] offsetY,
					input	logic	InsideRectangle, //input that the pixel is within a bracket 
					input logic [7:0] random_number, // input for random obstacle
					input logic [2:0] difficulty,
					input logic gameEnd,
					input logic gameStart,
					input logic gameRestart,
					
//------------------------input collision smiley and hart -student to complete functionality					
					input bird_collision,
					input bird_high_level_pig_collision,
			
					output	logic	pigDrawingRequest,// 1
					output	logic	twoHitPigDrawingRequest,// 2
					output	logic	woodenBoxDrawingRequest,// 3
					output	logic	eggDrawingRequest,// 4
					output	logic	stoneBoxDrawingRequest, // 5
					output	logic	steelBoxDrawingRequest, // 6
					output	logic	tntDrawingRequest, // 7
					output 	logic glassBoxDrawingRequest, // 8
					output 	logic brokenStoneBoxDrawingRequest, // 9
					output	logic	drawingRequest, // output that the pixel should be dispalyed 
					output	logic	[7:0] RGBout  // rgb value from the bitmap 
 ) ;
 
// Size represented as Number of X and Y bits 
localparam logic [7:0] TRANSPARENT_ENCODING = 8'h03 ;// RGB value in the bitmap representing a transparent pixel 
 /*  end generated by the tool */
// the screen is 640*480  or  20 * 15 squares of 32*32  bits ,  we wiil round usp to 16*16 and use only the top left 16*15 squares 
// this is the bitmap  of the maze , if there is a specific value  the  whole 32*32 rectange will be drawn on the screen
// there are  16 options of differents kinds of 32*32 squares 
// all numbers here are hard coded to simplify the  understanding 
logic [0:15] [0:15] [3:0]  MazeBitMapMask ; 

