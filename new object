	{8'h03,8'h03,8'h00,8'h2c,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h00,8'h03,8'h03},
	{8'h03,8'h00,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h74,8'h74,8'h74,8'h74,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h01,8'h03},
	{8'h03,8'h00,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h00,8'h03},
	{8'h02,8'h9d,8'h98,8'hff,8'hff,8'hff,8'h70,8'h9d,8'h9d,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h9d,8'h9d,8'h70,8'hff,8'hff,8'hff,8'h70,8'h9d,8'h9d,8'h00,8'h03},
	{8'h00,8'h9d,8'hff,8'hff,8'hff,8'hff,8'hff,8'h70,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h70,8'hff,8'hff,8'hff,8'hff,8'hff,8'h70,8'h9d,8'h9d,8'h00},
	{8'h00,8'h70,8'hff,8'hff,8'hff,8'hff,8'hff,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h9d,8'hff,8'hff,8'hff,8'hff,8'hff,8'hff,8'hff,8'h9d,8'h9d,8'h00},
	{8'h00,8'hff,8'hff,8'h00,8'h00,8'hff,8'hff,8'h74,8'hdc,8'hdc,8'h04,8'h04,8'h04,8'hdc,8'hdc,8'hdc,8'h04,8'h04,8'hdc,8'hdc,8'hdc,8'h74,8'hff,8'hff,8'hff,8'hff,8'h00,8'h24,8'hff,8'h9d,8'h9d,8'h00},
	{8'h00,8'hff,8'hff,8'hff,8'hff,8'hff,8'hff,8'h74,8'hdc,8'hdc,8'h04,8'h04,8'h04,8'hdc,8'hdc,8'h04,8'h04,8'h04,8'hdc,8'hdc,8'hdc,8'h74,8'hff,8'hff,8'hff,8'hff,8'hff,8'hff,8'hff,8'h9d,8'h9d,8'h00},
	{8'h00,8'h70,8'hff,8'hff,8'hff,8'hff,8'hff,8'hbc,8'hdc,8'hdc,8'h04,8'h04,8'h04,8'hdc,8'hdc,8'h04,8'h04,8'h04,8'hdc,8'hdc,8'hdc,8'h74,8'h70,8'hff,8'hff,8'hff,8'hff,8'hff,8'h70,8'h9d,8'h9d,8'h00},
	{8'h00,8'h78,8'h70,8'hff,8'hff,8'hff,8'hb9,8'h74,8'hdc,8'hdc,8'hdc,8'h04,8'hbc,8'hdc,8'hdc,8'hdc,8'h04,8'h94,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h70,8'hff,8'hff,8'hff,8'h70,8'h78,8'h9d,8'h9d,8'h00},
	{8'h00,8'h78,8'h78,8'h78,8'h70,8'h78,8'h78,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h9d,8'h9d,8'h7c,8'h78,8'h78,8'h78,8'h78,8'h9d,8'h9d,8'h7c,8'h00},
	{8'h03,8'h00,8'h9d,8'h9d,8'h78,8'h9d,8'h9d,8'h9d,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h00,8'h03},
	{8'h03,8'h00,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h74,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'hdc,8'h74,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h00,8'h03},
	{8'h03,8'h03,8'h00,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h7c,8'h74,8'h74,8'h74,8'h74,8'h74,8'h74,8'h7c,8'h7c,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h00,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h00,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h7c,8'h7c,8'h7c,8'h7c,8'h7c,8'h7c,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h00,8'h03,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h03,8'h00,8'h78,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h7c,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h00,8'h02,8'h03,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h03,8'h03,8'h00,8'h00,8'h78,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h04,8'h00,8'h03,8'h03,8'h03,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h00,8'h00,8'h00,8'h78,8'h78,8'h78,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h9d,8'h78,8'h78,8'h78,8'h00,8'h00,8'h00,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h01,8'h00,8'h00,8'h00,8'h78,8'h78,8'h78,8'h78,8'h78,8'h78,8'h78,8'h2c,8'h00,8'h00,8'h00,8'h02,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03},
	{8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h00,8'h00,8'h00,8'h00,8'h00,8'h00,8'h00,8'h00,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03,8'h03}
	}// pig
	};
 
// assign offsetY_LSB  = offsetY[4:0] ; // get lower 5 bits 
// assign offsetY_MSB  = offsetY[8:5] ; // get higher 4 bits 
// assign offsetX_LSB  = offsetX[4:0] ; 
// assign offsetX_MSB  = offsetX[8:5] ; 
// pipeline (ff) to get the pixel color from the array 	 

logic [0:15] [0:15] [3:0]  MazeDefaultBitMapMask= // defult table to load on reset 
{
{64'h0000000000000000},
{64'h0000000000000000},
{64'h0000000000000000},
{64'h0000000000000000}, 
{64'h0000000000000000},
{64'h0000000000000000}, 
{64'h0000000000000000},
{64'h0000000000000000},
{64'h0000000000000000},
{64'h0000000004000000}, 
{64'h0000000000300000},
{64'h0000000000000200}, 
{64'h0000000000601310},
{64'h0000000000000000}, 
{64'h0000000000000000},
{64'h0000000000000000}
};

logic gameRestarted;
logic flagEggHit;
logic flagPigHit;
logic flagKingPigHit;
logic flagWoodenBoxHit;
logic flagStoneBoxHit;
logic flagTntHit;
logic flagGlassBoxHit;
logic flagBrokenStoneBoxHit;
logic flagsteelBoxHit;


//==----------------------------------------------------------------------------------------------------------------=
always_ff@(posedge clk or negedge resetN)
begin
	if(!resetN) begin
		RGBout <=	8'h00;
		MazeBitMapMask <= MazeDefaultBitMapMask;
		gameRestarted <= 1;
		/*
		//pigDrawingRequest <= 0;// 1
		twoHitPigDrawingRequest <= 0;// 2
		woodenBoxDrawingRequest <= 0;// 3
		//eggDrawingRequest <= 0;// 4
		stoneBoxDrawingRequest <= 0;// 5
		steelBoxDrawingRequest <= 0; // 6
		tntDrawingRequest <= 0; // 7
		glassBoxDrawingRequest		<= 0; // 8
		brokenStoneBoxDrawingRequest <= 0;// 9
		*/
		flagPigHit <= 0;
		flagKingPigHit<=0;
		flagEggHit <= 0;
		flagWoodenBoxHit<= 0;
		flagStoneBoxHit<= 0;
		flagTntHit<= 0;
		flagGlassBoxHit<= 0;
		flagBrokenStoneBoxHit<= 0;
		flagsteelBoxHit<= 0;
	end
	
	else begin
	
		if(gameRestart == 1'b1) begin		
			gameRestarted <= 1;
		end
		
		if (gameStart == 1'b1 && gameRestarted == 1'b1) begin
			MazeBitMapMask <= MazeDefaultBitMapMask;
			gameRestarted <= 0;
			
			// load different map each time you start
			case(difficulty) 
				1 : MazeDefaultBitMapMask <= MazeDefaultBitMapMaskEasy[random_number%5];
				2 : MazeDefaultBitMapMask <= MazeDefaultBitMapMaskMedium[random_number%5];
				4 : MazeDefaultBitMapMask <= MazeDefaultBitMapMaskHard[random_number%5];
				default : MazeDefaultBitMapMask = MazeDefaultBitMapMaskMedium[0];
			endcase
			
			// add a randomly generated object to the matrix according to the diffculty level
			case(difficulty) 
				1 : MazeDefaultBitMapMask[random_number[7:4]][random_number[3:0]] <= 4; // egg
				
				2 : MazeDefaultBitMapMask[random_number[7:4]][random_number[3:0]] <= random_number %10;

				4 : MazeDefaultBitMapMask[random_number[7:4]][random_number[3:0]] <= random_number %10;

				default : MazeDefaultBitMapMask[random_number[7:4]][random_number[3:0]] <= 4;
			endcase
			
		end
			
		RGBout <= TRANSPARENT_ENCODING ; // default 
//----------------------------------add collision betwenn smiley and Hart -- disappear Hart  ------------------------------------------		
		if (bird_collision == 1'b1) begin
			case (MazeBitMapMask[offsetY[8:5]][offsetX[8:5]])
					 4'h4 : MazeBitMapMask[offsetY[8:5]][offsetX[8:5]] <= 4'h0; // egg
					 					 
					 4'h5 : MazeBitMapMask[offsetY[8:5]][offsetX[8:5]] <= 4'h9;  // stonebox

					 4'h6 : MazeBitMapMask[offsetY[8:5]][offsetX[8:5]] <= 4'h6; // steelbox
						
					 4'h2 : begin // high health pig turn into normal pig at a random place
								MazeBitMapMask[offsetY[8:5]][offsetX[8:5]] <= 4'h0; 
								MazeBitMapMask[random_number[7:4]][random_number[3:0]] <= 4'h1; 
							end
					 default: MazeBitMapMask[offsetY[8:5]][offsetX[8:5]] <= 4'h0 ;//delete object(,pig,wooden,tnt,glassbox)
			endcase
	    end 

		flagPigHit <= 0;
		flagKingPigHit<=0;
		flagEggHit <= 0;
		flagWoodenBoxHit<= 0;
		flagStoneBoxHit<= 0;
		flagTntHit<= 0;
		flagGlassBoxHit<= 0;
		flagBrokenStoneBoxHit<= 0;
		flagsteelBoxHit<= 0;
		
		
//------------------------------------End collision betwenn smiley and Hart-------------------------------------------- 		
		
		if (InsideRectangle == 1'b1)	
			begin 
		   	case (MazeBitMapMask[offsetY[8:5]][offsetX[8:5]])
					 4'h0 : RGBout <= TRANSPARENT_ENCODING ;//empty
					 
					 4'h1 : begin 
							RGBout <= object_colors[4'h0][offsetY[4:0]][offsetX[4:0]] ; // pig
							flagPigHit <=1;
							end
					 4'h2 : begin
							RGBout <= object_colors[4'h1][offsetY[4:0]][offsetX[4:0]] ; // twohitpig
							flagKingPigHit <=1;
							end
					 4'h3 : begin 
							RGBout <= object_colors[4'h2][offsetY[4:0]][offsetX[4:0]] ; // woodenbox
							flagWoodenBoxHit <= 1;
							end
					 4'h4 : begin 
							RGBout <= object_colors[4'h3][offsetY[4:0]][offsetX[4:0]] ; // egg 
							flagEggHit <=1;
							end
					 4'h5 : begin 
							RGBout <= object_colors[4'h4][offsetY[4:0]][offsetX[4:0]] ; // stonebox(2hit)
							flagStoneBoxHit <= 1;
							end
					 4'h6 : begin 
							RGBout <= object_colors[4'h5][offsetY[4:0]][offsetX[4:0]] ; // steelbox
							flagsteelBoxHit <=1;
							end
					 4'h7 : begin 
							RGBout <= object_colors[4'h6][offsetY[4:0]][offsetX[4:0]] ; // tnt
							flagTntHit <=1;
							end
					 4'h8 : begin 
							RGBout <= object_colors[4'h7][offsetY[4:0]][offsetX[4:0]] ; // glassbox
							flagGlassBoxHit<=1;
							end
					 4'h9 : begin 
							RGBout <= object_colors[4'h8][offsetY[4:0]][offsetX[4:0]] ; // brokenstonebox(1hit)
							flagBrokenStoneBoxHit<=1;
							end
							
					 default:  RGBout <= TRANSPARENT_ENCODING ; 
				endcase
			end 
 
	end 
end
//==----------------------------------------------------------------------------------------------------------------=
// decide if to draw the pixel or not 
assign drawingRequest = (RGBout != TRANSPARENT_ENCODING ) ? 1'b1 : 1'b0 ; // get optional transparent command from the bitmpap

assign eggDrawingRequest = (flagEggHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // egg

assign pigDrawingRequest = (flagPigHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // pig

assign twoHitPigDrawingRequest = (flagKingPigHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // king pig

assign woodenBoxDrawingRequest = (flagWoodenBoxHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // wooden box

assign stoneBoxDrawingRequest = (flagStoneBoxHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // stone box

assign steelBoxDrawingRequest = (flagsteelBoxHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // steel box

assign tntDrawingRequest = (flagTntHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // tnt 

assign glassBoxDrawingRequest = (flagGlassBoxHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // glass box 

assign brokenStoneBoxDrawingRequest = (flagBrokenStoneBoxHit && (RGBout != TRANSPARENT_ENCODING )) ? 1'b1 : 1'b0 ; // king pig


endmodule
